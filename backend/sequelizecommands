sequelize model:generate --name User --attributes username:string,passwordHash:string,admin:boolean

sequelize model:generate --name Post --attributes post:text,userId:integer

sequelize model:generate --name Comment --attributes postId:integer,comment:text,userId:integer

sequelize model:generate --name Photo --attributes imageUrl:string,userId:integer

sequelize model:generate --name Like --attributes postId:integer,userId:integer

sequelize model:generate --name Relationship --attributes userId:integer,userId2:integer,status:integer,actionUserId:integer



